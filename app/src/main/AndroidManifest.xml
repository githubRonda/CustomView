<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ronda.customview">

    <application
        android:allowBackup="true"
        android:name=".MyApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".youku.YoukuMenuActivity"/>
        <activity android:name=".banner.BannerActivity"/>
        <activity android:name=".dropedit.DropEditActivity"/>
        <activity android:name=".switchbutton.SwitchButtonActivity"/>
        <activity android:name=".listview.RefreshListViewActivity"/>
        <activity android:name=".sliding.SlidingActivity"/>
    </application>

</manifest>